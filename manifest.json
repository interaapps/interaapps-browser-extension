{
    "name": "InteraApps Accounts",
    "description": "InteraApps accounts addon",
    "version": "1.0.1",
    "author": "InteraApps",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
    ],
    "background": [
        "scripts/bg/background.js"
    ],
    "icons": {
        "32": "icon.png"
    }
  }